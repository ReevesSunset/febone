{
  "name": "febone-kit",
  "description": "the kit of febone",
  "author": "mx",
  "version": "0.8.17",
  "scripts": {
    "no": "no",
    "test": "no test",
    "prepublish": "no build"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@git.sankuai.com/mx/febone-kit.git"
  },
  "license": "MIT",
  "dependencies": {
    "path-to-regexp": "1.7.0",
    "send": "0.15.3",
    "webpack-simple-progress-plugin": "0.0.1"
  },
  "files": [
    "lib"
  ],
  "devDependencies": {
    "nokit": "0.25.1",
    "eslint": "3.12.0",
    "vane": "1.5.15",
    "webpack": "1.14.0"
  },
  "eslintConfig": {
    "rules": {
      "indent": [
        2,
        4
      ],
      "quotes": [
        2,
        "single"
      ],
      "linebreak-style": [
        2,
        "unix"
      ],
      "semi": [
        2,
        "always"
      ],
      "no-trailing-spaces": 2,
      "space-before-function-paren": [
        2,
        "always"
      ],
      "eqeqeq": [
        2,
        "allow-null"
      ],
      "no-console": 0,
      "no-debugger": 0
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "extends": "eslint:recommended"
  }
}
